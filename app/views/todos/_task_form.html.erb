
<%= form_for [@todo, :tasks] do |f| %>
    <%= @task_group.inspect %>
    <% @task_group.each do |task|%>
    <%= task.inspect %>
    <%= fields_for task do |tf| %>
     <%= tf.label :title %>
     <%= tf.text_field :title%>
    <% end %>
   <% end %><br />
      <%= f.submit "Create Tasks", :class => "btn btn-large btn-inverse"%>
<% end %>